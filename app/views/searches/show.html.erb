<h1>Search Result</h1>

<p><%= link_to 'Back', new_search_path %></p>

<% if @search.search_articles.empty? %>

  <p>No records found</p>
<% else %>

  <% @search.search_articles.each do |c| %>
  <br>

  <div class="div">
      <h1><%= c.title %></h1>
      <p><b>Journal</b> <%= c.journal %></p>
  </div>
<% end %>

<% end %>